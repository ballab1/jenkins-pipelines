
@Library('jenkins-sharedlibs')_

pipeline {
    agent {
        docker {
            label 'docker-host'
	    image 'jenkinsci/slave:3.16-1-alpine'
        }
    }
    stages {
        stage('checkout git') {
            steps {
                checkout scm
                sh 'env | sort'
            }
        }
        stage ('Publish to Kafka') {
            steps {
                kafkaPublish('docker_slave_test')
            }
        } 
    }
}
