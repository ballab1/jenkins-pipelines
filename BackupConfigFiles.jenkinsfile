
@Library('jenkins-sharedlibs')_

processInParallel (
    agent: 'master',
    items: activeNodes(),
    process: { nodeName, body -> 
        nodeStep(nodeName, body)
    },
    action: { nodeName ->
        def tarfile = nodeName + '.cfg.tgz'
        sh "tar -cvzf ${tarfile} -T ./tarfiles.lst ||:"
        sh "sudo cp ${tarfile} /home/bobb/src" 
        archiveArtifacts tarfile
    }
)