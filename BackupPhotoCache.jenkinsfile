
@Library('jenkins-sharedlibs')_

processInParallel (
    agent: 'ubuntu-s3',
    items: [ 'photoCache.tgz' ],
    process: { tarfile, body -> 
        fixedNodeStep(tarfile, body)
    },
    action: { tarfile ->
        sh "tar czf '${WORKSPACE}/${tarfile}' -C /home/bobb/production/workspace.production/zen cache ||:"
        sh "sudo cp '${WORKSPACE}/${tarfile}' /home/bobb/xsrc/"
        archiveArtifacts tarfile
    }
)