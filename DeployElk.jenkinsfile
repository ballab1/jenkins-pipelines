@Library('jenkins-sharedlibs')_

/////////////////////////////////////////////////////////////////////////////////////////////////

pipeline {
  agent { label 'master' }

  stages {
    stage ('ubuntu-s5') {
      agent { label 'ubuntu-s5' }
      steps {
        ansiColor('xterm') {
	  updateWorkspace ('/home/bobb/elk-deploy', ['./deploy', '-l'])
        }
      }
    }
  }
  post {
    always {
        echo 'Publish to Kafka'
        kafkaPublish('elk_deploy')
    }
  }
} 