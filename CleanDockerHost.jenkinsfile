
@Library('jenkins-sharedlibs')_

processInParallel (
    agent: 'master',
    items: activeNodes(),
    process: { nodeName, body -> 
        nodeStep(nodeName, body)
    },
    action: { nodeName ->
        ansiColor('xterm') {
            sh '''
                  if [ $(hostname) != 'ubuntu-s2' ]; then
                      ./updateBin.sh
                      ./docker-utilities clean
                  fi
                '''
        }
    }
)