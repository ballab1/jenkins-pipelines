
@Library('jenkins-sharedlibs')_

processInParallel (
    agent: 'master',
    items: [ 'ubuntu-s1', 'ubuntu-s3', 'ubuntu-s4', 'ubuntu-s5', 'ubuntu-s6' ],
    process: { nodeName, body -> 
        nodeStep(nodeName, body)
    },
    action: { nodeName ->
        sh '''
              ./updateBin.sh
              ./docker-utilities clean
           '''
    }
)