
@Library('jenkins-sharedlibs')_

processInParallel (
    agent: 'master',
    items: [ 'ubuntu-s1', 'ubuntu-s3', 'ubuntu-s4', 'ubuntu-s5' ],
    process: { nodeName, body -> 
        nodeStep(nodeName, body)
    },
    action: { nodeName ->
        sh 'bin/docker-utilities clean'
    },
    topic: 'clean_docker_host'
)