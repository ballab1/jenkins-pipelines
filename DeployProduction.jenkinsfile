node('s3.ubuntu.home') {
    timestamps {
        stage ('Perform Update') {
            ws {
                sh "DOCKER_REGISTRY='registry.ubuntu.home/' CONTAINER_TAG='latest' docker-compose -f ${PROD_DIR}/docker-compose.yml pull"
            }
        }
        stage ('Restart Production') {
            ws {
                withEnv([PATH+'EXTRA=/home/bobb/bin']) {
                    sh "restartProd ${PROD_DIR}"
                }
            }
        }
    }
}
